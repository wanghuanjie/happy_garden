<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ziyoujiayuan.happygarden.dao.def.ApplyFertilizerRecordDefDAO">

  <select id="listByCondition" parameterType="com.ziyoujiayuan.happygarden.param.cultivate.ApplyFertilizerQueryParam" resultType="com.ziyoujiayuan.happygarden.entity.def.ApplyFertilizerRecordDefPO">
     SELECT *
       FROM t_apply_fertilizer_record t
      WHERE t.is_del = 0
      <if test="flowerId != null">
          AND t.flower_id = #{flowerId}
      </if>
      <if test="creator != null">
        AND t.creator = #{creator}
      </if>
  </select>

</mapper>